#include<stdio.h>
int main()
{
int m,n,a[100],b[100],c[100],i,j,k=0;
printf("enter the size of first array:");
scanf("%d",&m);
printf("\n enter the elements in array:");
for(i=0;i<m;i++)
{
scanf("%d",&a[i]);
c[i]=a[i];
}
k=i;
printf("\nenter the size of scenod array:");
scanf("%d",&n);
printf("\n enter the elements in array:");
for(i=0;i<n;i++)
{
scanf("%d",&b[i]);
c[k]=b[i];
k++;
}
 for(i=0;i<k;i++)
 printf("\n %d",c[i]);
 return 0;
}
